{
  "name": "agricultural-trading-system",
  "version": "1.0.0",
  "description": "A full-stack web application that connects farmers with buyers, enabling efficient trading of agricultural products",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd BACKEND && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "install:all": "cd BACKEND && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "dev:backend": "cd BACKEND && npm run dev"
  },
  "keywords": [
    "agricultural",
    "trading",
    "marketplace",
    "farmers",
    "react",
    "express",
    "mongodb"
  ],
  "author": "Prageeth Gunarathne",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "react-router-dom": "^6.30.0"
  }
}
